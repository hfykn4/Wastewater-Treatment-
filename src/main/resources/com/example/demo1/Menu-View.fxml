<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.MenuController">
   <children>
      <AnchorPane prefHeight="90.0" prefWidth="600.0" style="-fx-background-color: #00D4FF;">
         <children>
            <Label fx:id="MainMenu" layoutX="127.0" layoutY="31.0" prefHeight="18.0" prefWidth="220.0" text="Main Menu ">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <ImageView fitHeight="90.0" fitWidth="102.0" layoutX="21.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../wastewater/Menu.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <Button fx:id="addButton" layoutX="218.0" layoutY="167.0" mnemonicParsing="false" onAction="#addButtonOnAction" prefHeight="26.0" prefWidth="140.0" style="-fx-background-color: #00D4FF;" text="Add New Model" />
      <Button fx:id="ManageButton" layoutX="218.0" layoutY="211.0" mnemonicParsing="false" onAction="#manageButtonOnAction" prefHeight="26.0" prefWidth="140.0" style="-fx-background-color: #00D4FF;" text="Manage Model" />
      <Button fx:id="AfterMenu" layoutX="477.0" layoutY="351.0" mnemonicParsing="false" onAction="#afterMenuButtonOnAction" prefHeight="26.0" prefWidth="96.0" style="-fx-background-color: #00D4FF;" text="Continue" />
      <Button fx:id="SelectButton" layoutX="218.0" layoutY="254.0" mnemonicParsing="false" onAction="#selectButtonOnAction" prefHeight="26.0" prefWidth="140.0" style="-fx-background-color: #00D4FF;" text="Model Selection" />
   </children>
</AnchorPane>
